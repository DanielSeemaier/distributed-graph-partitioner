#!/bin/bash 
. "$script_pwd/../systems/generic"

# i10*: runs the experiment through `exclusive`

GenerateJobfileSubmission() {
    for file in ${@}; do 
        echo "bash $file" >> "$submit_impl_filename"
    done
}

GenerateJobfileWrapper() {
    echo "nohup exclusive bash -- $submit_impl_filename &"
    echo "disown"
}
