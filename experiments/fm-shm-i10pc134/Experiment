DefineAlgorithm dKaMinPar-FM5-NoPremove        dKaMinPar --r-algorithm=prob-lp+fm --r-fm-radius=2 --r-fm-iterations=5 --r-fm-premove-locally=0
DefineAlgorithm dKaMinPar-FM5-SeqOverlap       dKaMinPar --r-algorithm=prob-lp+fm --r-fm-radius=2 --r-fm-iterations=5 --r-fm-overlap-regions=1 --r-fm-sequential=1
DefineAlgorithm dKaMinPar-FM5-SampleSeqOverlap dKaMinPar --r-algorithm=prob-lp+fm --r-fm-radius=2 --r-fm-iterations=5 --r-fm-overlap-regions=1 --r-fm-sequential=1 --r-fm-bound-degree=1000

Algorithms dKaMinPar dKaMinPar-FM1 dKaMinPar-FM5 dKaMinPar-FM5-NoPremove dKaMinPar-FM5-SeqOverlap dKaMinPar-FM5-SampleSeqOverlap

